    "lint": "eslint ."
  },
  "dependencies": {
    "@astrojs/check": "^0.5.9",
    "@astrojs/partytown": "^2.0.4",
    "@astrojs/rss": "^4.0.5",
    "@resvg/resvg-js": "^2.6.0",
    "astro": "^4.5.6",
    "fuse.js": "^7.0.0",
    "github-slugger": "^2.0.0",
    "markdown-it": "^14.1.0",
    "rehype-external-links": "^3.0.0",
    "remark-collapse": "^0.1.2",
    "remark-toc": "^9.0.0",
    "sanitize-html": "^2.12.1",
    "satori": "^0.10.13",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.2"
  },
  "devDependencies": {
    "@astrojs/react": "^3.1.0",
    "@astrojs/sitemap": "^3.1.1",
    "@astrojs/tailwind": "^5.1.0",
    "@divriots/jampack": "^0.23.3",
