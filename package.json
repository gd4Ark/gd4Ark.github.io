{
  "name": "astro-paper",
  "version": "4.2.0",
  "packageManager": "pnpm@8.15.4",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build && jampack ./dist",
    "preview": "astro preview",
    "sync": "astro sync",
